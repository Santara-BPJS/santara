{
  "name": "@santara/bot",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "santara-bot": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile bot"
  },
  "dependencies": {
    "@effect/ai": "^0.32.1",
    "@effect/ai-google": "^0.11.0",
    "@effect/cli": "^0.72.1",
    "@effect/platform": "^0.93.2",
    "@effect/platform-bun": "^0.83.0",
    "@effect/platform-node": "^0.100.0",
    "@santara/db": "workspace:*",
    "effect": "^3.19.3",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.34.2"
  },
  "devDependencies": {
    "@types/bun": "^1.3.2",
    "@types/qrcode-terminal": "^0.12.2"
  }
}
